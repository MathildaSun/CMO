{
  "name": "tiprun-marketing-agents",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:setup": "tsx scripts/setup-db.ts",
    "test": "vitest run",
    "format": "prettier -w .",
    "smoke": "tsx scripts/smoke.ts",
    "ci": "npm run lint && npm run typecheck && npm test"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@slack/web-api": "^7.13.0",
    "@supabase/supabase-js": "^2.44.3",
    "bullmq": "^4.18.3",
    "dotenv": "^16.4.5",
    "hono": "^4.11.7",
    "ioredis": "^5.9.2",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.67.0",
    "pg": "^8.11.5",
    "resend": "^3.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^1.5.3"
  }
}
